#include <iostream>
using namespace std;

class Card {

public:
	string rank;
	string suit;

	Card(string r, string s) {
		rank = r;
		suit = s;
	}

	string getRank() {
		return rank;
	}

	string getSuit() {
		return suit;
	}
};

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
