#include <string>
#include "Hand.h"

using namespace std;

char playerRank[5];
string playerSuit[5];

Hand::Hand(char rank[5], string suit[5]) {

	playerRank = rank;
	playerSuit = suit;

}

char* getRank() {
	return playerRank;
}


